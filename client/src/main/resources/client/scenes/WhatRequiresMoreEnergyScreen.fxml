<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane id="root" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.SplashCtrl">
  <center>
    <VBox alignment="CENTER" maxWidth="640" BorderPane.alignment="CENTER">
         <Button fx:id="backButton" onAction="#back" styleClass="border, rounded, drop-shadow" text="Exit" />
      <Region prefHeight="0.0" prefWidth="640.0" styleClass="margin" VBox.vgrow="ALWAYS" />
      <Text styleClass="question-title" text="What requires more energy?" textAlignment="CENTER" BorderPane.alignment="CENTER" />
         <HBox prefHeight="212.0" prefWidth="1140.0">
            <children>
               <Button fx:id="button0" mnemonicParsing="false" HBox.hgrow="ALWAYS">
                  <styleClass>
                     <String fx:value="border" />
                     <String fx:value="rounded" />
                     <String fx:value="drop-shadow" />
                  </styleClass></Button>
               <Button fx:id="button1" mnemonicParsing="false" HBox.hgrow="ALWAYS">
                  <styleClass>
                     <String fx:value="border" />
                     <String fx:value="rounded" />
                     <String fx:value="drop-shadow" />
                  </styleClass>
               </Button>
               <Button fx:id="button2" mnemonicParsing="false" HBox.hgrow="ALWAYS">
                  <styleClass>
                     <String fx:value="border" />
                     <String fx:value="rounded" />
                     <String fx:value="drop-shadow" />
                  </styleClass>
               </Button>
            </children>
         </HBox>
      <Region styleClass="margin" VBox.vgrow="ALWAYS" />
    </VBox>
  </center>
   <stylesheets>
      <URL value="@../css/main.css" />
      <URL value="@../css/WhatRequiresMoreEnergy.css" />
   </stylesheets>
</BorderPane>
